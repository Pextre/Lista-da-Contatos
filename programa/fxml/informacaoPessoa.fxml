<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.DialogPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>

<?import javafx.scene.layout.GridPane?>
<DialogPane xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="programa.controladores.InformacaoPessoa">
    <content>
        <GridPane hgap="10" vgap="10">
            <Label text="Nome" GridPane.rowIndex="0" GridPane.columnIndex="0"/>
            <TextField fx:id="novoNome" promptText="Nome" GridPane.rowIndex="0" GridPane.columnIndex="1"/>
            <Label text="Sobrenome" GridPane.rowIndex="1" GridPane.columnIndex="0"/>
            <TextField fx:id="novoSobrenome" promptText="Sobrenome" GridPane.rowIndex="1" GridPane.columnIndex="1"/>
            <Label text="Celular" GridPane.rowIndex="2" GridPane.columnIndex="0"/>
            <TextField fx:id="novoCelular" promptText="Celular" GridPane.rowIndex="2" GridPane.columnIndex="1"/>
            <Label text="Nota" GridPane.rowIndex="3" GridPane.columnIndex="0"/>
            <TextField fx:id="novaNota" promptText="Nota" GridPane.rowIndex="3" GridPane.columnIndex="1"/>
        </GridPane>

    </content>


</DialogPane>
